{
  "name": "instant-quote-builder",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x || 20.x"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/supabase-js": "^2.76.1",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "dotenv": "^17.2.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "resend": "^6.3.0",
    "terser-webpack-plugin": "5.3.14",
    "typescript": "^4.9.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "postinstall": "npm rebuild terser-webpack-plugin || npm install terser-webpack-plugin@5.3.14 --save-exact --force || true",
    "prebuild": "npm rebuild terser-webpack-plugin 2>/dev/null || (npm install terser-webpack-plugin@5.3.14 --save-exact --force && cd node_modules/terser-webpack-plugin && npm install --production=false && npm run build && cd ../..) || true",
    "build": "node -e \"require('terser-webpack-plugin')\" || (npm rebuild terser-webpack-plugin && cd node_modules/terser-webpack-plugin && npm install && npm run build && cd ../..) && react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0"
  },
  "overrides": {
    "terser-webpack-plugin": "5.3.14"
  }
}
