# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit your actual .env file to git!

# ===========================================
# REQUIRED - Core Services
# ===========================================

# Supabase Configuration (Required for database)
REACT_APP_SUPABASE_URL=your_supabase_project_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OpenAI API Key (Required for AI furniture detection)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Optional: GPT Model Selection (defaults to gpt-4-turbo-preview)
REACT_APP_GPT_MODEL=gpt-4-turbo-preview

# ===========================================
# OPTIONAL - Email Services
# ===========================================

# Option 1: Resend (Recommended for PDF emails)
REACT_APP_RESEND_API_KEY=your_resend_api_key_here

# Option 2: EmailJS (Alternative)
REACT_APP_EMAILJS_SERVICE_ID=your_emailjs_service_id_here
REACT_APP_EMAILJS_TEMPLATE_ID=your_emailjs_template_id_here
REACT_APP_EMAILJS_PUBLIC_KEY=your_emailjs_public_key_here

# ===========================================
# OPTIONAL - Google Services
# ===========================================

# Google Maps API (for distance calculation)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Vision API (alternative to OpenAI)
REACT_APP_GOOGLE_VISION_API_KEY=your_google_vision_api_key_here
